# Gemini API key (used for synthesis + audio transcription)
GEMINI_API_KEY=your-gemini-api-key

# YouTube Data API key (optional, used for search fallback)
YOUTUBE_API_KEY=your-youtube-api-key

# Serper API key (Google search provider)
SERPER_API_KEY=your-serper-api-key

# Webshare proxy (for YouTube transcript fetching)
WEBSHARE_PROXY_URL=http://user:pass@proxy-host:port

# Path to ffmpeg directory (local dev only; Docker image has ffmpeg built-in)
FFMPEG_LOCATION=
